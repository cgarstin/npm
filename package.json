{
    "name": "@gik/npm",
    "version": "0.1.3",
    "description": "Scripts for your EcmaScript workflow.",
    "license": "MIT",
    "homepage": "https://github.com/gikmx/npm",
    "contributors": [
        "Héctor Menéndez <hector@gik.mx>"
    ],
    "keywords": [
        "npm",
        "yarn",
        "script",
        "scripts",
        "npm scripts",
        "yarn scripts"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gikmx/npm.git"
    },
    "bugs": {
        "url": "https://github.com/gikmx/npm/issues"
    },
    "os": [
        "linux",
        "darwin"
    ],
    "engines": {
        "node": ">=6.11.3",
        "npm": ">=5.4.2"
    },
    "bin": {
        "gik-npm": "./lib/bin.js"
    },
    "directories": {
        "src": "./src",
        "out": "./lib"
    },
    "scripts": {
        "command:lint": "eslint",
        "command:babel": "rm -Rf ./lib && babel --config-file ./.babelrc -d ./lib ./src && chmod +x ./lib/bin.js",
        "ver": "./lib/bin.js version",
        "test": "./lib/bin.js test && chmod +x ./lib/bin.js",
        "lint": "./lib/bin.js lint",
        "docs": "./lib/bin.js docs",
        "start": "npm run command:babel -s && ./lib/bin.js start",
        "build": "npm run command:babel -s && ./lib/bin.js build && chmod +x ./lib/bin.js",
        "verify": "npm run build -s && npm run lint src -s && npm run test -s",
        "precommit": "npm run verify -s && npm run ver -s $([ -z $VER ] && echo patch || echo $VER) && npm run docs -s",
        "pub:beta": "npm run verify -s && npm run docs -s && npm publish --tag beta",
        "pub:latest": "npm run verify -s && npm run docs -s && npm publish --tag latest"
    },
    "peerDependencies": {},
    "dependencies": {
        "@gik/tools-streamer": "0.0.7",
        "babel-cli": "^7.0.0-beta.3",
        "babel-core": "^7.0.0-beta.3",
        "babel-plugin-syntax-dynamic-import": "^7.0.0-beta.3",
        "babel-polyfill": "^7.0.0-beta.3",
        "babel-preset-env": "^7.0.0-beta.3",
        "babel-preset-stage-2": "^7.0.0-beta.3",
        "babel-register": "^7.0.0-beta.3",
        "chalk": "^2.1.0",
        "debug": "^3.1.0",
        "deepmerge": "^2.0.0",
        "eslint": "^4.8.0",
        "handlebars": "^4.0.11",
        "husky": "^0.14.3",
        "jest": "^21.3.0-beta.4",
        "jest-cli": "^21.2.1",
        "jsdoc-babel": "^0.3.0",
        "jsdoc-to-markdown": "^3.0.0",
        "nodegit": "^0.20.2",
        "nodemon": "^1.12.1",
        "npm": "^5.4.2",
        "read-package-json": "^2.0.12",
        "shelljs": "^0.7.8",
        "source-map-support": "^0.5.0",
        "tmp": "0.0.33"
    },
    "devDependencies": {
        "@gik/eslint-config-node": "0.0.3"
    },
    "jest": {
        "verbose": false
    },
    "@gik/npm": {
        "jsdoc": {
            "private": false
        }
    }
}
